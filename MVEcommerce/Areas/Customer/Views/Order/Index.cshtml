                <h3 id="order_review_heading">Your order</h3>

                <div id="order_review" class="woocommerce-checkout-review-order">
                    <table class="shop_table woocommerce-checkout-review-order-table">
                        <thead>
                            <tr>
                                <th class="product-name">Product</th>
                                <th class="product-total">Subtotal</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{
                                var vendorGroups = Model.Carts.GroupBy(c => c.Product.Vendor.Name);
                                foreach (var vendorGroup in vendorGroups)
                                {
                                    <tr>
                                        <td colspan="2"><strong>Vendor: @vendorGroup.Key</strong></td>
                                    </tr>
                                    foreach (var cart in vendorGroup)
                                    {
                                        var price = cart.VariantOptionID.HasValue ? cart.ProductVariantOption.Price : cart.Product.Price;
                                        var sale = cart.VariantOptionID.HasValue ? cart.ProductVariantOption.Sale : cart.Product.Sale;
                                        var finalPrice = price - (price * sale / 100);

                                        <tr class="cart_item">
                                            <td class="product-name">
                                                @cart.Product.Name&nbsp; <strong class="product-quantity">×&nbsp;@cart.Quantity</strong>
                                                <dl class="variation">
                                                    <dt class="variation-Vendor">Vendor:</dt>
                                                    <dd class="variation-Vendor">
                                                        <p>@cart.Product.Vendor.Name</p>
                                                    </dd>
                                                </dl>
                                            </td>
                                            <td class="product-total">
                                                <span class="woocs_special_price_code">
                                                    <span class="woocommerce-Price-amount amount">
                                                        <bdi>
                                                            <span class="woocommerce-Price-currencySymbol">$</span>@(finalPrice * cart.Quantity)
                                                        </bdi>
                                                    </span>
                                                </span>
                                            </td>
                                        </tr>
                                    }
                                    <tr class="cart-subtotal">
                                        <th>        <em>Subtotal for @vendorGroup.Key</em></th>
                                        <td>
                                            <span class="woocommerce-Price-amount amount">
                                                <bdi>
                                                    <span class="woocommerce-Price-currencySymbol">$</span>@vendorGroup.Sum(c => (c.VariantOptionID.HasValue ? (c.ProductVariantOption.Price - (c.ProductVariantOption.Price * c.ProductVariantOption.Sale / 100)) : (c.Product.Price - (c.Product.Price * c.Product.Sale / 100))) * c.Quantity)
                                                </bdi>
                                            </span>
                                        </td>
                                    </tr>
                                }
                            }
                        </tbody>
                        <tfoot>
                            <tr class="order-total">
                                <th>Total</th>
                                <td>
                                    <strong>
                                        <span class="woocommerce-Price-amount amount">
                                            <bdi>
                                                <span class="woocommerce-Price-currencySymbol">$</span>@Model.Carts.Sum(c => (c.VariantOptionID.HasValue ? (c.ProductVariantOption.Price - (c.ProductVariantOption.Price * c.ProductVariantOption.Sale / 100)) : (c.Product.Price - (c.Product.Price * c.Product.Sale / 100))) * c.Quantity)
                                            </bdi>
                                        </span>
                                    </strong>
                                </td>
                            </tr>
                        </tfoot>
